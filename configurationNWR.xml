<?xml version="1.0" encoding="UTF-8"?>

<cf:Configuration
	xmlns:cf="http://www.sg.ethz.ch/research/cuttlefish/configuration.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.sg.ethz.ch/research/cuttlefish/configuration.xsd src/ch/ethz/sg/cuttlefish/resources/configuration.xsd ">
	<GUI>
		<Tab id="data" condition="*" name="data">
			<Widget id="source" name="Source"
				class="ch.ethz.sg.cuttlefish.gui.widgets.DataSourcePanel">
				<Argument name="sources">datasourcesNWR.xml</Argument>
			</Widget>
			
			<Widget id="networkt" name="Networkt"
				class="ch.ethz.sg.Main.gui.NetworkPanel">
				<Argument name="nwtype">BB,SCC,NW</Argument>
			</Widget>
			
			<Widget id="country" name="Country"
				class="ch.ethz.sg.Main.gui.CountryPanel">
				<!--
				<Argument name="countries">AU,BM,BR,CA,CH,CN,DE,ES,FI,FR,GB,GR,HK,ID,IL,IN,IT,JP,KR,KY,NO,PE,PH,PL,PT,RU,SE,SG,TH,TW,US,ZA,CHTot</Argument>
				-->
				<Argument name="countries">AE,AR,AT,AU,BE,BM,CA,CH,CL,CN,DE,DK,ES,FI,FR,GB,GR,HK,ID,IE,IL,IN,IS,IT,JO,JP,KR,KW,KY,LU,MX,MY,NL,NO,NZ,OM,PH,PT,SA,SE,SG,TH,TN,TR,TW,US,VG,ZA</Argument>
			</Widget>
			
			<Widget id="export" name="export"
				class="ch.ethz.sg.cuttlefish.gui.widgets.ExportPanel" />
			<Widget id="animation" name="animation"
				class="ch.ethz.sg.cuttlefish.gui.widgets.AnimationPanel">
				<Argument name="folder">animations</Argument></Widget>
		</Tab>

		<Tab id="network" condition="*" name="network">
			<Widget id="metrics" name="metrics"
				class="ch.ethz.sg.cuttlefish.gui.widgets.NetworkMetricPanel" />
			<Widget id="edges" name="edges"
				class="ch.ethz.sg.cuttlefish.gui.widgets.EdgePanel" />
			<Widget id="date" name="date"
				class="ch.ethz.sg.cuttlefish.gui.widgets.DatePanel" />
		</Tab>


		<Tab name="simulation"
			condition="ch.ethz.sg.cuttlefish.networks.ISimulation"
			id="simulation">
			<Widget name="control"
				class="ch.ethz.sg.cuttlefish.gui.widgets.SimulationPanel"
				id="control" />
		</Tab>
	</GUI>
	<Arguments>
		<Argument name="layoutFolder">pos/</Argument>
	</Arguments>
</cf:Configuration>
